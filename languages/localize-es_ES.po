#: templates/settings.php:8
#@ localize
msgid "Localize WordPress"
msgstr "Localize WordPress"

#: templates/settings.php:11
#@ localize
msgid "Introduction"
msgstr "Introducción"

#: templates/settings.php:13
#@ localize
msgid "This plugin will help you enable localization for your language on this WordPress installation."
msgstr "Este plugin le ayudará a activar la posición para su idioma en esta instalación de WordPress."

#: templates/settings.php:14
#@ localize
msgid "All you need to do is select the language code from the list below, and the version you want to use."
msgstr "Todo lo que necesita hacer es seleccionar el código de idioma de la siguiente lista y la versión que desea utilizar."

#: templates/settings.php:15
#@ localize
msgid "The <strong>stable version</strong> will load the file from already published translations."
msgstr "La <strong>versión estable</strong> cargará el archivo con las traducciones ya publicadas."

#: templates/settings.php:17
#@ localize
msgid "The <strong>development version</strong> will try to download the file directly from "
msgstr "La <strong>versión en desarrollo</strong> intentará descargar el archivo directamente desde"

#: templates/settings.php:24
#@ localize
msgid "Options"
msgstr "Opciones"

#: templates/settings.php:26
#@ localize
msgid "Please select your language and it's version:"
msgstr "Por favor seleccione su idioma y su versión:"

#: templates/settings.php:30
#@ localize
msgid "Localization Code"
msgstr "Código de localización"

#: templates/settings.php:32
#@ localize
msgid "The localization code is composed of two letters language code, an underscore and two letters country code."
msgstr "El código de localización está compuesto por el código de dos letras del idioma, un guión bajo y el código de dos letras del país. Ejemplo: es_ES para el Español."

#: templates/settings.php:35
#@ localize
msgid "Follow these links to find out your localization code: "
msgstr "Siga estos enlaces para obtener el código de localización:"

#: templates/settings.php:37
#@ localize
msgid "Language Codes List"
msgstr "Lista de códigos de Idiomas"

#: templates/settings.php:40
#@ localize
msgid "Country Codes List"
msgstr "Lista de códigos de Paises"

